package Assessment2;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		for (int i = n * 2; i >= 1; i--) {
			for (int j = 1; j <= i; j = j + 2) {
				System.out.print("*");
			}
			System.out.print("\n");
		}
	}

}
